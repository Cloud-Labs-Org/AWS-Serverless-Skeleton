{
  "name": "aws-serverless-skeleton",
  "version": "1.0.0",
  "description": "AWS Serverless Framework Skeleton for fast start",
  "author": "Cloud-Labs",
  "scripts": {
    "format:check": "prettier --config ./.prettierrc --list-different \"src/**/*{.ts,.js,.json}\" --write",
    "quality:check": "npm run format:check && ./node_modules/tslint/bin/tslint --project ./tsconfig.json --config ./tslint.json",
    "test": "nyc ts-mocha --paths -p ./tsconfig.spec.json -r ts-node/register test/**/*.spec.ts",
    "coverage": "nyc --reporter=html --reporter=text --reporter=text-lcov > coverage.lcov -r lcov -e .ts -x \"*.spec.ts\" npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cloud-Labs-Org/AWS-Serverless-Skeleton"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/aws-lambda": "^8.10.17",
    "@types/chai": "^4.2.7",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.12.18",
    "aws-sdk": "^2.486.0",
    "chai": "^4.2.0",
    "lint-staged": "^9.2.1",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "serverless-iam-roles-per-function": "^2.0.2",
    "serverless-webpack": "^5.2.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.16",
    "terser-webpack-plugin": "^2.2.1",
    "ts-loader": "^5.3.3",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.5.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.18.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2"
  }
}
