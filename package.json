{
  "name": "aws-serverless-skeleton",
  "version": "1.0.0",
  "description": "AWS Serverless Framework Skeleton for fast start",
  "author": "Cloud-Labs",
  "scripts": {
    "format:check": "prettier --config ./.prettierrc --list-different \"src/**/*{.ts,.js,.json}\" --write",
    "quality:check": "npm run format:check && ./node_modules/tslint/bin/tslint --project ./tsconfig.json --config ./tslint.json",
    "test": "jest -c ./jest.config.js ",
    "coverage": "npm test --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cloud-Labs-Org/AWS-Serverless-Skeleton"
  },
  "license": "MIT",
  "keywords": [
    "serverless",
    "serverless framework",
    "aws",
    "amazon",
    "cloudformation",
    "sam",
    "skeleton",
    "cognito",
    "apigateway",
    "apigatewayauthorizer",
    "s3"
  ],
  "dependencies": {},
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/aws-lambda": "^8.10.17",
    "@types/jest": "^24.0.23",
    "@types/node": "^10.12.18",
    "aws-sdk": "^2.486.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.1",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "serverless-iam-roles-per-function": "^2.0.2",
    "serverless-webpack": "^5.2.0",
    "source-map-support": "^0.5.16",
    "terser-webpack-plugin": "^2.2.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.5.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.18.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2"
